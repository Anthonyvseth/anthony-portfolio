io\r\n                                </NavLinks>\r\n                            </NavItem>\r\n                            <NavItem>\r\n                                <NavLinks \r\n                                    to='skills'\r\n                                    smooth={true}\r\n                                    duration={500}\r\n                                    spy={true}\r\n                                    exact='true'\r\n                                    offset={-80}\r\n                                > \r\n                                    <FaCode /> Skills\r\n                                </NavLinks>\r\n                            </NavItem>\r\n                            <NavItem>\r\n                                <NavLinks\r\n                                    to='contact'\r\n                                    smooth={true}\r\n                                    duration={500}\r\n                                    spy={true}\r\n                                    exact='true'\r\n                                    offset={-80}\r\n                                 > \r\n                                    <FaAddressBook /> Contact\r\n                                 </NavLinks>\r\n                            </NavItem>\r\n                        </NavMenu>\r\n                    </NavBarContainer>\r\n                </Nav>\r\n            </IconContext.Provider>\r\n    )\r\n}\r\n\r\nexport default NavBar\r\n","/Users/Anthonyvseth/Documents/GitHub/anthony-portfolio/client/src/components/about/index.js",[],"/Users/Anthonyvseth/Documents/GitHub/anthony-portfolio/client/src/components/contactSection/index.js",["109","110","111"],"import React, {Component} from 'react'\r\nimport {\r\n    VideoBg,\r\n    ContContainer,\r\n    FormWrap,\r\n    Form,\r\n    ContBg,\r\n    FormContent,\r\n    FormInput,\r\n    FormLabel,\r\n    ContH1,\r\n    ContContent,\r\n    FormButton, FormMessage, SpanErr\r\n} from './ContactElems'\r\nimport Video from '../.